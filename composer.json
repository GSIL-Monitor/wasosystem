{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "anlutro/l4-settings": "^0.7.3",
        "arcanedev/log-viewer": "4.5.*",
        "awssat/laravel-visits": "^1.4",
        "dingo/api": "^2.0.0-alpha2",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "^4.0",
        "geoip2/geoip2": "^2.9",
        "intervention/image": "^2.4",
        "kalnoy/nestedset": "^4.3",
        "laravel/framework": "5.6.*",
        "laravel/socialite": "^3.2",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.6",
        "maatwebsite/excel": "^3.0",
        "mews/captcha": "^2.1",
        "overtrue/easy-sms": "^1.0",
        "overtrue/laravel-lang": "~3.0",
        "overtrue/laravel-ueditor": "~1.0",
        "overtrue/pinyin": "~3.0",
        "phpoffice/phpword": "^0.15.0",
        "predis/predis": "^1.1",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "socialiteproviders/weixin": "^4.0",
        "spatie/laravel-permission": "^2.12",
        "torann/geoip": "^1.0",
        "wangju/ding-notice": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "barryvdh/laravel-ide-helper": "^2.4",
        "beyondcode/laravel-query-detector": "^0.4.1",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "summerblue/generator": "^0.6.0",
        "xethron/migrations-generator": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files":[
            "app/Helpers/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
